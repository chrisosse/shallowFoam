FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application shallowFoam;

//startFrom       startTime;
startForm       0;

startTime       0;

//stopAt	        writeNow;
stopAt          endTime;

endTime         2;

deltaT          0.005;

writeControl    adjustableRunTime;
//writeControl    timeStep;

writeInterval   0.05;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.2;

