FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volScalarField;
    object          H;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    leftWall
    {
        type       zeroGradient;
    }

    rightWall
    {
        type       fixedValue;
        value      uniform 2;
    }

    bottomWall
    {
        type       empty;
    }

    atmosphere
    {
        type       empty;
    }

    sides
    {
        type       empty;
    }

}


