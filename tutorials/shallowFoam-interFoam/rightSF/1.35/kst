FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.35";
    object      kst;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 30;

boundaryField
{
    leftWall
    {
        type            fixedValue;
        value           uniform 30;
    }
    rightWall
    {
        type            fixedValue;
        value           uniform 30;
    }
    lowerWall
    {
        type            empty;
    }
    atmosphere
    {
        type            empty;
    }
    sides
    {
        type            empty;
    }
}


